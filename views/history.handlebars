<style>
    .icDash {
        background: none;
    }

    .icDash a {
        color: #434343;
    }

    .iconIcDash {
        filter: invert(100%) sepia(0%) saturate(0%) hue-rotate(0deg) brightness(50%) contrast(60%);
    }

    .iconIcHist {
        filter: none;
    }

    .icHist {
        background: var(--linear-cardbg, linear-gradient(151deg, #3D6E9C 14.5%, #3E91AD 96.35%));
    }

    .icHist a {
        color: #fff;
    }

    .alternatePageDash {
        display: block;
    }

    .alternatePageHist {
        display: none;
    }

    .outHist {
        width: 9px;
        height: 14px;
    }

    .outDash {
        width: 0;
        height: 0;
    }
</style>
<main class="mainDash">
    <header class="headerDash">
        <p class="titlePage">histórico Geral</p>
        <input type="search" name="search" id="" class="searchBar" placeholder="Pesquise aqui...">
        <article class="profileCont">
            <img src="/assets/icons/arrowPfp.svg" class="arrow" alt="" srcset="">
            <p class="profileContTitle">
                Rafael Souza
            </p>
            <img src="/assets/img/pfp/pfp1.png" alt="" class="pfp" srcset="">
        </article>
    </header>
    <section class="lastChanges lc2">
        <header class="headLastChanges">

            <section class="buttonsLastChanges">
                <button class="addBtn btn" id="addBtn">Adicionar</button>
            </section>
        </header>
        <main class="listLastChanges">
            <figure class="table">
                <article class="itemTb">
                    <p class="titleColumn">
                        Acomodação
                    </p>
                    <p class="titleColumn">
                        Nome do Hópede
                    </p>
                    <p class="titleColumn">
                        Data de Entrada
                    </p>
                    <p class="titleColumn">
                        Data de Saída
                    </p>
                    <p class="titleColumn">
                        Status da reserva
                    </p>
                    <p class="titleColumn">
                        ID
                    </p>
                </article>
                <hr style="margin-top: 10px;">
            </figure>
            {{#each reservas}}
            <figure class="table">
                <article class="itemTb">
                    <p class="titleItem">
                        {{this.nome_acomodacao}}
                    </p>
                    <p class="titleItem">
                        {{this.nome_hospede}}
                    </p>
                    <p class="titleItem">
                        {{this.data_entrada}}
                    </p>
                    <p class="titleItem">
                        {{this.data_saida}}
                    </p>
                    <p class="titleItem">
                        {{this.status_reserva}}
                    </p>
                    <p class="titleItem">
                        {{this.id}}
                    </p>
                </article>
            </figure>
            {{/each}}
        </main>
    </section>
</main>